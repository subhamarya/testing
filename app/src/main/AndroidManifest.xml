<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.subhamarya.javaprogramming">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/jf"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Tutorials"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Programs"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Important_Questions"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Quiz"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".About_Us"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".Splash"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WebActivity" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".web2" android:theme="@style/Theme.AppCompat.NoActionBar"></activity>
    </application>

</manifest>